{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "HashmapADF"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "HashmapCopyBlobtoSF",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "SnowflakeSink",
								"importSettings": {
									"type": "SnowflakeImportCopyCommand",
									"additionalCopyOptions": {
										"FORCE": "FALSE"
									}
								}
							},
							"enableStaging": false
						},
						"inputs": [
							{
								"referenceName": "HashmapADFCSV",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "HashmapADFSFTable",
								"type": "DatasetReference"
							}
						]
					},
					{
						"name": "HashmapADFCopyData",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "SnowflakeSink",
								"importSettings": {
									"type": "SnowflakeImportCopyCommand",
									"additionalCopyOptions": {
										"FORCE": "FALSE"
									}
								}
							},
							"enableStaging": false
						},
						"inputs": [
							{
								"referenceName": "HashmapADFCSV",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "HashmapADFSFTable",
								"type": "DatasetReference"
							}
						]
					},
					{
						"name": "Lookup1",
						"type": "Lookup",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "SnowflakeSource",
								"query": " select * from CITY_BIKE_DATA",
								"exportSettings": {
									"type": "SnowflakeExportCopyCommand"
								}
							},
							"dataset": {
								"referenceName": "HashmapADFSFTable",
								"type": "DatasetReference"
							}
						}
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/HashmapADFCSV')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/HashmapADFCSV')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "hashmapADFBlobLinkedService",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "long-202005-citibike-tripdata.csv",
						"folderPath": "test",
						"container": "hashmapadfcontainer"
					},
					"columnDelimiter": ",",
					"rowDelimiter": "\r\n",
					"escapeChar": "\\",
					"firstRowAsHeader": false,
					"quoteChar": ""
				},
				"schema": [
					{
						"name": "tripduration",
						"type": "String"
					},
					{
						"name": "starttime",
						"type": "String"
					},
					{
						"name": "stoptime",
						"type": "String"
					},
					{
						"name": "start station id",
						"type": "String"
					},
					{
						"name": "start station name",
						"type": "String"
					},
					{
						"name": "start station latitude",
						"type": "String"
					},
					{
						"name": "start station longitude",
						"type": "String"
					},
					{
						"name": "end station id",
						"type": "String"
					},
					{
						"name": "end station name",
						"type": "String"
					},
					{
						"name": "end station latitude",
						"type": "String"
					},
					{
						"name": "end station longitude",
						"type": "String"
					},
					{
						"name": "bikeid",
						"type": "String"
					},
					{
						"name": "usertype",
						"type": "String"
					},
					{
						"name": "birth year",
						"type": "String"
					},
					{
						"name": "gender",
						"type": "String"
					}
				]
			},
			"dependsOn": []
		}
	]
}