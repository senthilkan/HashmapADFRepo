{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "HashmapADF"
		},
		"AZUSenthilKVLinkedService_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://AZUSenthilKV.vault.azure.net/"
		},
		"HashmapADFSFLinkedSerice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'HashmapADFSFLinkedSerice'"
		},
		"hashmapADFBlobLinkedService_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'hashmapADFBlobLinkedService'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AZUSenthilKVLinkedService_properties_typeProperties_baseUrl": {
						"value": "[parameters('AZUSenthilKVLinkedService_properties_typeProperties_baseUrl')]"
					},
					"HashmapADFSFLinkedSerice_connectionString": {
						"value": "[parameters('HashmapADFSFLinkedSerice_connectionString')]"
					},
					"hashmapADFBlobLinkedService_sasUri": {
						"value": "[parameters('hashmapADFBlobLinkedService_sasUri')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}